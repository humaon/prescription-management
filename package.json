    "node-cron": "^4.2.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pdf-parse-new": "^1.4.1",
    "pdfkit": "^0.17.2"
